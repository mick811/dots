{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "Rosé Pine Moon",
  "author": "Rose Pine",
  "themes": [
    {
      "name": "Rosé Pine Moon",
      "appearance": "dark",
      "style": {
        "background": "#000000",
        "foreground": "#e0def4",
        "border": "#403d52",
        "editor.background": "#000000",
        "editor.foreground": "#e0def4",
        "editor.cursor": "#e0def4",
        "editor.selection": "#403d52",
        "editor.line_highlight": "#191724",
        "editor.gutter": "#6e6a86",
        "editor.line_number": "#6e6a86",
        "editor.accent": "#31748f",
        "syntax": {
          "comment": { "color": "#6e6a86" },
          "keyword": { "color": "#31748f" },
          "string": { "color": "#f6c177" },
          "function": { "color": "#9ccfd8" },
          "variable": { "color": "#e0def4" },
          "constant": { "color": "#c4a7e7" },
          "type": { "color": "#ebbcba" },
          "operator": { "color": "#eb6f92" },
          "punctuation": { "color": "#908caa" },
          "tag": { "color": "#eb6f92" },
          "attribute": { "color": "#f6c177" },
          "number": { "color": "#c4a7e7" },
          "class": { "color": "#ebbcba" },
          "interface": { "color": "#ebbcba" },
          "enum": { "color": "#ebbcba" },
          "struct": { "color": "#ebbcba" },
          "type_parameter": { "color": "#ebbcba" },
          "parameter": { "color": "#e0def4" },
          "property": { "color": "#9ccfd8" },
          "namespace": { "color": "#c4a7e7" },
          "module": { "color": "#c4a7e7" },
          "macro": { "color": "#eb6f92" },
          "builtin": { "color": "#c4a7e7" },
          "escape": { "color": "#eb6f92" },
          "regex": { "color": "#ebbcba" },
          "symbol": { "color": "#f6c177" },
          "boolean": { "color": "#c4a7e7" },
          "annotation": { "color": "#eb6f92" }
        }
      }
    }
  ]
}
